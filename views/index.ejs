<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= sub %></p>
    <table>
      <% items.forEach((item) => { %>
      <tr>
        <td>
            <%= item.id %>
        </td>
        <td>
            <%= item.Term %>
        </td>
        <td>
            <%= item.Description %>
        </td>
        <td>
            <%= item.Reference %>
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
